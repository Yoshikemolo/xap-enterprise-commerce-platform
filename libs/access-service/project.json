{
  "name": "access-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "root": "libs/access-service",
  "sourceRoot": "libs/access-service/src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/access-service",
        "main": "libs/access-service/src/main.ts",
        "tsConfig": "libs/access-service/tsconfig.app.json",
        "assets": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "libs/access-service/src/environments/environment.ts",
              "with": "libs/access-service/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "access-service:build",
        "port": 3001
      },
      "configurations": {
        "production": {
          "buildTarget": "access-service:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/access-service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/access-service"],
      "options": {
        "jestConfig": "libs/access-service/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:backend", "type:service", "domain:access"]
}
